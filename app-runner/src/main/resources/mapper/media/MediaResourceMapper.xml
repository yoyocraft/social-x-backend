<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youyi.domain.media.repository.mapper.MediaResourceMapper">

    <resultMap id="UserInfoResultMap" type="com.youyi.domain.media.repository.po.MediaResourcePO">
        <id column="id" property="id"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
        <result property="deletedAt" column="deleted_at"/>
        <result property="resourceKey" column="resource_key"/>
        <result property="resourceType" column="resource_type"/>
        <result property="resourceUrl" column="resource_url"/>
        <result property="source" column="source"/>
        <result property="creatorId" column="creator_id"/>
    </resultMap>

    <sql id="result_cols">
        id, gmt_create, gmt_modified, deleted_at, extra_data, resource_key, resource_type, resource_url, source,
        creator_id
    </sql>
    <insert id="insert">
        INSERT INTO media_resource (extra_data, resource_key, resource_type, resource_url, source, creator_id)
        VALUES (#{extraData}, #{resourceKey}, #{resourceType}, #{resourceUrl}, #{source}, #{creatorId})
    </insert>


</mapper>
